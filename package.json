{
  "name": "blossom-token-wrapper-monorepo",
  "private": true,
  "version": "0.0.1",
  "devDependencies": {
    "@changesets/cli": "^2.25.2"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "changeset:release": "yarn build:connector && changeset publish",
    "changeset:version": "changeset version",
    "build": "yarn build:connector && yarn build:aragon-app",
    "build:connector": "cd packages/connector && yarn build",
    "build:aragon-app": "cd packages/aragon-app && yarn build"
  }
}